{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "VibeTravelsDatabase": "${DATABASE_URL}"
  },
  "JwtSettings": {
    "SecretKey": "${JWT_SECRET}",
    "Issuer": "VibeTravels",
    "Audience": "VibeTravels",
    "ExpirationInMinutes": 60
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "Cors": {
    "AllowedOrigins": [ "https://your-production-domain.com" ]
  },
  "OpenAi": {
    "Enabled": false,
    "ApiKey": "${OPENAI_API_KEY}",
    "Model": "gpt-4o-mini",
    "Temperature": 1,
    "MaxTokens": 10000
  }
}
